{
  "name": "energy-ts",
  "private": true,
  "version": "0.0.0",
  "exports": "./dist/index.js",
  "prisma": {
    "schema": "./server/prisma/schema.prisma"
  },
  "scripts": {
    "dev": "npm run dev:frontend && npm run dev:server",
    "pm": "dotenv -e .env -- npx prisma migrate dev",
    "dev:frontend": "vite ./frontend",
    "dev:server": "nodemon ./server/index.ts",
    "d-restart": "docker-compose down --rmi local && docker volume prune -f && docker-compose up -d",
    "d-logs": "docker-compose logs -f --tail=100",
    "d-down": "docker-compose down --rmi local && docker volume prune -f ",
    "build": "npm i && npm run build:frontend && npm run build:server && docker build . -t energyapp",
    "build:frontend": "vue-tsc -p ./frontend/tsconfig.json --noEmit && vite build ./frontend --emptyOutDir",
    "build:server": "npx tsc -p ./server/tsconfig.json",
    "preview": "vite preview"
  },
  "dependencies": {
    "@popperjs/core": "^2.11.6",
    "@prisma/client": "^4.9.0",
    "@zxing/library": "^0.19.1",
    "async-mqtt": "^2.6.3",
    "axios": "^0.27.2",
    "base64url": "^3.0.1",
    "bootstrap": "^5.2.1",
    "cbor": "^8.1.0",
    "chart.js": "^3.9.1",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "dotenv-cli": "^7.0.0",
    "express": "^4.18.1",
    "jsonwebtoken": "^8.5.1",
    "mssql": "^9.0.1",
    "pinia": "^2.0.0-rc.10",
    "pinia-plugin-persist": "^1.0.0",
    "vue": "^3.2.37",
    "vue-chartjs": "^4.1.2",
    "vue-router": "^4.1.5"
  },
  "devDependencies": {
    "@types/cbor": "^6.0.0",
    "@types/cookie-parser": "^1.4.3",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.14",
    "@types/jsonwebtoken": "^8.5.9",
    "@types/mssql": "^8.1.1",
    "@types/node": "^18.7.23",
    "@vitejs/plugin-vue": "^3.1.0",
    "autoprefixer": "^10.4.13",
    "nodemon": "^2.0.20",
    "postcss": "^8.4.21",
    "sass": "^1.55.0",
    "tailwindcss": "^3.2.4",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.3",
    "vite": "^3.1.0",
    "vue-tsc": "^0.40.4"
  }
}
